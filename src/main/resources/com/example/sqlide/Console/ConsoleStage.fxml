<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox fx:id="Container" style="-fx-background-color: #2A2A2A;" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sqlide.Console.ConsoleController">

    <children>
        <ToolBar style="-fx-background-color: #2C2C2C;">
            <items>
                <Label text="Terminal:" textFill="#f2f2f2">
                    <font>
                        <Font size="16.0" />
                    </font>
                </Label>
                <ChoiceBox fx:id="ModeBox" prefWidth="150.0" style="-fx-background-color: #2C2C2C;" stylesheets="@../../../../css/ChoiceBoxModern.css" />
                <JFXButton mnemonicParsing="false" onAction="#delete" style="-fx-background-color: #D21209; -fx-background-radius: 5px;" text="Delete" textFill="WHITESMOKE" />
            </items>
        </ToolBar>
        <Separator style="-fx-base: black;" />
        <ScrollPane fx:id="Scroll" fitToHeight="true" fitToWidth="true" stylesheets="@../../../../css/ScrollHbarStyle.css" VBox.vgrow="ALWAYS" />
    </children>

</VBox>
