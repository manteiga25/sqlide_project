<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="625.0" prefWidth="600.0" style="-fx-background-color: #2B2D30;" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sqlide.EventLayout.EditEventController">
   <children>
      <Label alignment="CENTER" layoutX="211.0" layoutY="14.0" prefHeight="32.0" prefWidth="197.0" text="Edit Event" textFill="WHITE">
