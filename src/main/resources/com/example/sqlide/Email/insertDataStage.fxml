<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="5.0" style="-fx-background-color: #2C2C2C;" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sqlide.Email.insertDataController">
   <children>
      <HBox alignment="CENTER" spacing="5.0" stylesheets="@../../../../css/ChoiceBoxModern.css">
         <children>
            <Label text="Table:" textFill="#f2f2f2">
               <font>
                  <Font name="Arial" size="16.0" />
               </font></Label>
            <ChoiceBox fx:id="TableBox" prefWidth="150.0" style="-fx-background-color: #2C2C2C;">
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin></ChoiceBox>
            <Label text="Column:" textFill="#f2f2f2">
               <font>
                  <Font name="Arial" size="16.0" />
               </font></Label>
            <ChoiceBox fx:id="ColumnBox" prefWidth="150.0" style="-fx-background-color: #2C2C2C;" />
         </children>
         <VBox.margin>
            <Insets left="15.0" right="15.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER_RIGHT" spacing="5.0">
         <children>
            <JFXButton mnemonicParsing="false" onAction="#close" style="-fx-background-color: #3574F0;" text="Cancel" textFill="#f5f5f5">
               <font>
                  <Font name="Arial" size="12.0" />
               </font></JFXButton>
            <JFXButton mnemonicParsing="false" onAction="#addData" style="-fx-background-color: #3574F0;" text="Confirm" textFill="#f5f5f5">
               <font>
                  <Font name="Arial" size="12.0" />
               </font></JFXButton>
         </children>
         <padding>
            <Insets bottom="5.0" right="5.0" />
         </padding>
      </HBox>
   </children>
</VBox>
