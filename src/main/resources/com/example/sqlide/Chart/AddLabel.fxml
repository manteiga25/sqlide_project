<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox spacing="10.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sqlide.Chart.ChartLabel">
   <children>
      <HBox alignment="CENTER" spacing="5.0">
         <children>
            <Label text="Name/Group:" />
            <TextField fx:id="NameField" />
         </children>
      </HBox>
      <HBox alignment="CENTER" spacing="5.0">
         <children>
            <Label text="Category (X):" />
            <TextField fx:id="CategoryField" />
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutX="10.0" layoutY="15.0" spacing="5.0">
         <children>
            <Label text="Column:" />
            <ChoiceBox fx:id="ColumnBox" prefWidth="150.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER" spacing="5.0">
         <children>
            <Label text="Function:" />
            <ChoiceBox fx:id="FuncBox" prefWidth="150.0" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <HBox alignment="TOP_RIGHT" spacing="5.0">
         <children>
            <Button mnemonicParsing="false" onAction="#confirm" text="Create" />
         </children>
         <padding>
            <Insets bottom="5.0" />
         </padding>
      </HBox>
   </children>
   <padding>
      <Insets left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
